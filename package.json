{
  "name": "dig-trace-parser",
  "version": "0.0.10",
  "description": "dig command wrapper created by node",
  "main": "index.js",
  "type": "index.d.ts",
  "scripts": {
    "test": "xo && nyc ava test/*.js"
  },
  "keywords": [
    "domain",
    "dns",
    "dig",
    "whois"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sunecosuri/dig-trace-parser.git"
  },
  "author": "sunecosuri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunecosuri/dig-trace-parser/issues"
  },
  "homepage": "https://github.com/sunecosuri/dig-trace-parser#readme",
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "padding-line-between-statements": [
        "error",
        { "blankLine": "any", "prev": "*", "next": "return"}
      ],
      "no-negated-condition": "off",
      "no-useless-escape": "off",
      "unicorn/prefer-starts-ends-with": "off",
      "no-await-in-loop": "off"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.4.1",
    "nyc": "15.1.0",
    "xo": "^0.58.0"
  }
}
